{
  "name": "youtube_channels",
  "description": "Verwaltung von YouTube-Kanälen für automatische Video-Verarbeitung",
  "columns": [
    {
      "name": "channel_id",
      "type": "text",
      "required": true,
      "unique": true,
      "description": "YouTube Channel ID (aus Channel-URL)"
    },
    {
      "name": "channel_name",
      "type": "text",
      "required": true,
      "description": "Anzeigename des Kanals"
    },
    {
      "name": "channel_url",
      "type": "text",
      "required": true,
      "description": "Vollständige YouTube-URL (youtube.com/@username)"
    },
    {
      "name": "original_language",
      "type": "text",
      "required": true,
      "description": "ISO 639-1 Sprachcode (en, de, es, fr, etc.)"
    },
    {
      "name": "enabled",
      "type": "boolean",
      "required": true,
      "default": true,
      "description": "Kanal aktiv? Nur aktive Kanäle werden verarbeitet"
    },
    {
      "name": "notebook_name",
      "type": "text",
      "required": true,
      "description": "Name des Open Notebook Notebooks (z.B. 'YT: Kanalname')"
    },
    {
      "name": "notebook_id",
      "type": "text",
      "required": false,
      "description": "Open Notebook ID (wird automatisch vom Workflow gesetzt)"
    },
    {
      "name": "last_sync",
      "type": "datetime",
      "required": false,
      "description": "Zeitpunkt des letzten Video-Scans"
    },
    {
      "name": "video_count",
      "type": "number",
      "required": false,
      "default": 0,
      "description": "Anzahl verarbeiteter Videos"
    },
    {
      "name": "created_at",
      "type": "datetime",
      "required": false,
      "auto": "created",
      "description": "Zeitpunkt der Erstellung"
    }
  ],
  "example_data": [
    {
      "channel_id": "UCXuqSBlHAE6Xw-yeJA0Tunw",
      "channel_name": "Linus Tech Tips",
      "channel_url": "youtube.com/@LinusTechTips",
      "original_language": "en",
      "enabled": true,
      "notebook_name": "YT: Linus Tech Tips",
      "notebook_id": null,
      "last_sync": null,
      "video_count": 0
    },
    {
      "channel_id": "UCZYTClx2T1of7BRZ86-8fow",
      "channel_name": "SciShow",
      "channel_url": "youtube.com/@SciShow",
      "original_language": "en",
      "enabled": true,
      "notebook_name": "YT: SciShow",
      "notebook_id": null,
      "last_sync": null,
      "video_count": 0
    }
  ],
  "notes": "Füge Channels manuell in diese Table ein. Der Workflow verarbeitet alle Channels mit enabled=true"
}
